{
  "name": "tiddlywiki-ipfs",
  "version": "0.2.2",
  "description": "Ipfs with TiddlyWiki",
  "main": "ipfs-library.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "prepare": "./scripts/prepare.sh",
    "browserify": "browserify src/ipfs-library.js -s IpfsLibrary -o ipfs/plugins/ipfs/ipfs-library.js",
    "tiddlywiki-ipfs": "tiddlywiki ipfs --output ipfs/output/base --build --verbose",
    "tiddlywiki-ipfs-cardo": "tiddlywiki ipfs --import tw5-cardo/Cardo-1.0.4.json application/json --output ipfs/output/cardo --build --verbose",
    "all": "./scripts/all.sh",
    "all-dev": "./scripts/all-dev.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xmaysonnave/tiddlywiki-ipfs.git"
  },
  "keywords": [
    "ipfs",
    "tiddlywiki"
  ],
  "author": "Xavier Maysonnave",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/xmaysonnave/tiddlywiki-ipfs/issues"
  },
  "homepage": "https://github.com/xmaysonnave/tiddlywiki-ipfs",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "browserify-derequire": "^1.0.1",
    "browserify-unibabel": "^3.0.0",
    "buffer": "^5.4.3",
    "cids": "^0.7.1",
    "content-hash": "^2.4.4",
    "@ensdomains/resolver": "^0.1.11",
    "ethers": "^4.0.38",
    "ipfs-provider": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "browserify-transform-tools": "^1.7.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "prettier": "^1.18.2",
    "source-map": "^0.7.3",
    "source-map-explorer": "^2.1.0",
    "tiddlywiki": "^5.1.21"
  }
}
