{
  "name": "tiddlywiki-ipfs",
  "version": "0.1.0",
  "description": "Ipfs for TiddlyWiki",
  "main": "ipfs-saver.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "prepare": "./scripts/prepare.sh",
    "browserify": "browserify src/ipfs-saver.js -s ipfsSaver -o ipfs/plugins/ipfs/ipfs-saver.js",
    "tiddlywiki-ipfs": "tiddlywiki ipfs --import attachment/the_little_prince_embedded.json application/json --import attachment/the_little_prince_ipfs_encrypted.json application/json --import attachment/the_little_prince_ipfs.json application/json --output ipfs/output/base --build --verbose",
    "tiddlywiki-ipfs-cardo": "tiddlywiki ipfs --import attachment/the_little_prince_embedded.json application/json --import attachment/the_little_prince_ipfs_encrypted.json application/json --import attachment/the_little_prince_ipfs.json application/json --import tw5-cardo/Cardo-1.0.4.json application/json --output ipfs/output/cardo --build --verbose",
    "all": "./scripts/all.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xmaysonnave/tiddlywiki-ipfs.git"
  },
  "keywords": [
    "ipfs",
    "tiddlywiki"
  ],
  "author": "Xavier Maysonnave",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/xmaysonnave/tiddlywiki-ipfs/issues"
  },
  "homepage": "https://github.com/xmaysonnave/tiddlywiki-ipfs",
  "dependencies": {
    "ipfs-provider": "^0.2.2",
    "cids": "^0.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-array-last-index": "^0.0.4",
    "babel-plugin-flow-runtime": "^0.19.0",
    "source-map-loader": "^0.2.4",
    "prettier": "^1.18.2",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "tiddlywiki": "^5.1.21",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0"
  }
}
