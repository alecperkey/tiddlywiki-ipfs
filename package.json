{
  "name": "tiddlywiki-ipfs",
  "version": "v0.3.0-200318399",
  "description": "IPFS with TiddlyWiki",
  "main": "ipfs-bundle.js",
  "directories": {
    "test": "test"
  },
  "browser": {
    "universal-url": "universal-url-lite"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.js'"
    }
  },
  "scripts": {
    "build-all-noclone": "./scripts/build-all-noclone.sh",
    "build": "./scripts/build.sh",
    "build-tiddlywiki": "./scripts/build-tiddlywiki.sh",
    "eslint-check": "eslint --print-config src/ipfs-saver.js | eslint-config-prettier-check",
    "lint": "eslint \"./src/**/*.js\"",
    "pretty-quick": "pretty-quick --verbose --pattern '**/*.js'",
    "run-build-number": "./scripts/run-build-number.sh",
    "run-browserify": "./scripts/run-browserify.sh",
    "run-prepare-clone": "./scripts/run-prepare-clone.sh",
    "run-prepare": "./scripts/run-prepare.sh",
    "test": "./scripts/test.sh",
    "test-no-build": "jest --verbose ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xmaysonnave/tiddlywiki-ipfs.git"
  },
  "keywords": [
    "ens",
    "ipfs",
    "tiddlywiki"
  ],
  "author": "Xavier Maysonnave",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/xmaysonnave/tiddlywiki-ipfs/issues"
  },
  "homepage": "https://github.com/xmaysonnave/tiddlywiki-ipfs",
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "buffer": "^5.5.0",
    "cids": "^0.7.4",
    "content-hash": "^2.5.2",
    "ipfs-provider": "git+https://github.com/xmaysonnave/ipfs-provider.git#v0.2.9",
    "universal-url": "^2.0.0",
    "universal-url-lite": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "acorn": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "browserify-transform-tools": "^1.7.0",
    "build-number-generator": "0.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "loglevel": "^1.6.7",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "tiddlywiki": "^5.1.21",
    "window-or-global": "^1.0.1"
  }
}